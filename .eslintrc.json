{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "linebreak-style": "off",
      "import/no-unresolved": "off",
      "class-methods-use-this": "off",
      "import/extensions": "off",
      "no-console": "off",
      "import/prefer-default-export": "off",
      "comma-dangle": "off",
      "max-len": [
        "warn",
        200
      ],
      "no-underscore-dangle": "off"
    }
}
